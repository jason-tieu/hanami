<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0B0B0D"/>
  <rect x="20" y="20" width="360" height="260" rx="12" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  
  <!-- Canvas LMS Integrator Interface -->
  <rect x="40" y="40" width="320" height="180" rx="8" fill="#0F0F0F"/>
  
  <!-- Header -->
  <rect x="50" y="50" width="300" height="30" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="200" y="70" fill="#FF4B8A" font-family="sans-serif" font-size="14" font-weight="bold" text-anchor="middle">Canvas LMS Integrator</text>
  
  <!-- OAuth2 Flow -->
  <rect x="60" y="90" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="100" y="105" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">OAuth2</text>
  <text x="100" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Authentication</text>
  
  <!-- Arrow -->
  <path d="M150 110 L170 110" stroke="#FF4B8A" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Canvas API -->
  <rect x="180" y="90" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="220" y="105" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Canvas</text>
  <text x="220" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">REST API</text>
  
  <!-- Arrow -->
  <path d="M270 110 L290 110" stroke="#FF4B8A" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Data Processing -->
  <rect x="300" y="90" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="340" y="105" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Data</text>
  <text x="340" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Processing</text>
  
  <!-- Course Data Display -->
  <rect x="60" y="150" width="320" height="60" rx="4" fill="#1A1A1A"/>
  
  <!-- Course Item 1 -->
  <rect x="70" y="160" width="300" height="15" rx="2" fill="#2A2A4A"/>
  <text x="75" y="170" fill="#FF4B8A" font-family="monospace" font-size="8">CAB320 - Algorithms & Complexity</text>
  <text x="320" y="170" fill="#999" font-family="monospace" font-size="6">Due: 2025-03-15</text>
  
  <!-- Course Item 2 -->
  <rect x="70" y="180" width="300" height="15" rx="2" fill="#2A2A4A"/>
  <text x="75" y="190" fill="#FF4B8A" font-family="monospace" font-size="8">EGH445 - Control Systems</text>
  <text x="320" y="190" fill="#999" font-family="monospace" font-size="6">Due: 2025-03-20</text>
  
  <!-- Course Item 3 -->
  <rect x="70" y="200" width="300" height="15" rx="2" fill="#2A2A4A"/>
  <text x="75" y="210" fill="#FF4B8A" font-family="monospace" font-size="8">CAB420 - Machine Learning</text>
  <text x="320" y="210" fill="#999" font-family="monospace" font-size="6">Due: 2025-03-25</text>
  
  <!-- Features -->
  <rect x="60" y="220" width="100" height="20" rx="2" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="110" y="232" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Study Planner</text>
  
  <rect x="170" y="220" width="100" height="20" rx="2" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="220" y="232" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Calendar Sync</text>
  
  <rect x="280" y="220" width="100" height="20" rx="2" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="330" y="232" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Rubric Analysis</text>
  
  <!-- Title -->
  <text x="200" y="250" fill="#FF4B8A" font-family="sans-serif" font-size="16" font-weight="bold" text-anchor="middle">Canvas LMS Integrator</text>
  <text x="200" y="270" fill="#999" font-family="sans-serif" font-size="12" text-anchor="middle">Next.js • OAuth2 • Canvas API</text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#FF4B8A"/>
    </marker>
  </defs>
</svg>
