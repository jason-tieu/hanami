<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0B0B0D"/>
  <rect x="20" y="20" width="360" height="260" rx="12" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  
  <!-- Maglev Control System -->
  <rect x="40" y="40" width="320" height="180" rx="8" fill="#0F0F0F"/>
  
  <!-- Maglev Platform -->
  <rect x="60" y="100" width="280" height="20" rx="10" fill="#2A2A4A" stroke="#FF4B8A" stroke-width="2"/>
  <rect x="80" y="80" width="40" height="40" rx="4" fill="#1A1A2E" stroke="#FF4B8A" stroke-width="1"/>
  <rect x="140" y="80" width="40" height="40" rx="4" fill="#1A1A2E" stroke="#FF4B8A" stroke-width="1"/>
  <rect x="200" y="80" width="40" height="40" rx="4" fill="#1A1A2E" stroke="#FF4B8A" stroke-width="1"/>
  <rect x="260" y="80" width="40" height="40" rx="4" fill="#1A1A2E" stroke="#FF4B8A" stroke-width="1"/>
  
  <!-- Magnetic Field Lines -->
  <path d="M100 60 Q150 40 200 60" stroke="#FF4B8A" stroke-width="1" fill="none" opacity="0.6"/>
  <path d="M120 70 Q150 50 180 70" stroke="#FF4B8A" stroke-width="1" fill="none" opacity="0.6"/>
  <path d="M140 80 Q150 60 160 80" stroke="#FF4B8A" stroke-width="1" fill="none" opacity="0.6"/>
  
  <!-- Control System Diagram -->
  <rect x="60" y="140" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="100" y="155" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">DLQR</text>
  <text x="100" y="170" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Controller</text>
  
  <rect x="160" y="140" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="200" y="155" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Kalman</text>
  <text x="200" y="170" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Filter</text>
  
  <rect x="260" y="140" width="80" height="40" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="300" y="155" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Observer</text>
  <text x="300" y="170" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Design</text>
  
  <!-- Connection Lines -->
  <path d="M140 160 L160 160" stroke="#FF4B8A" stroke-width="1" stroke-dasharray="2,2"/>
  <path d="M240 160 L260 160" stroke="#FF4B8A" stroke-width="1" stroke-dasharray="2,2"/>
  
  <!-- MATLAB/Simulink Logo -->
  <rect x="60" y="190" width="40" height="20" rx="2" fill="#FF4B8A" opacity="0.2"/>
  <text x="80" y="202" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">MATLAB</text>
  
  <rect x="110" y="190" width="40" height="20" rx="2" fill="#FF4B8A" opacity="0.2"/>
  <text x="130" y="202" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">Simulink</text>
  
  <!-- Title -->
  <text x="200" y="250" fill="#FF4B8A" font-family="sans-serif" font-size="16" font-weight="bold" text-anchor="middle">Maglev Control System</text>
  <text x="200" y="270" fill="#999" font-family="sans-serif" font-size="12" text-anchor="middle">MATLAB • DLQR • Kalman Filter • Control Theory</text>
</svg>
