<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0B0B0D"/>
  <rect x="20" y="20" width="360" height="260" rx="12" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  
  <!-- Job Tracker Interface -->
  <rect x="40" y="40" width="320" height="180" rx="8" fill="#0F0F0F"/>
  
  <!-- Header -->
  <rect x="50" y="50" width="300" height="30" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <text x="200" y="70" fill="#FF4B8A" font-family="sans-serif" font-size="14" font-weight="bold" text-anchor="middle">Job Application Tracker</text>
  
  <!-- Filter Bar -->
  <rect x="50" y="90" width="300" height="25" rx="4" fill="#2A2A4A"/>
  <rect x="55" y="95" width="60" height="15" rx="2" fill="#FF4B8A" opacity="0.3"/>
  <text x="85" y="105" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">All</text>
  <rect x="125" y="95" width="60" height="15" rx="2" fill="#333"/>
  <text x="155" y="105" fill="#999" font-family="monospace" font-size="8" text-anchor="middle">Applied</text>
  <rect x="195" y="95" width="60" height="15" rx="2" fill="#333"/>
  <text x="225" y="105" fill="#999" font-family="monospace" font-size="8" text-anchor="middle">Interview</text>
  <rect x="265" y="95" width="60" height="15" rx="2" fill="#333"/>
  <text x="295" y="105" fill="#999" font-family="monospace" font-size="8" text-anchor="middle">Rejected</text>
  
  <!-- Job List -->
  <rect x="50" y="125" width="300" height="80" rx="4" fill="#1A1A1A"/>
  
  <!-- Job Item 1 -->
  <rect x="55" y="130" width="290" height="20" rx="2" fill="#2A2A4A"/>
  <text x="60" y="142" fill="#FF4B8A" font-family="monospace" font-size="8">Google - Software Engineer</text>
  <circle cx="320" cy="140" r="3" fill="#00D4AA"/>
  <text x="330" y="142" fill="#999" font-family="monospace" font-size="6">Applied</text>
  
  <!-- Job Item 2 -->
  <rect x="55" y="155" width="290" height="20" rx="2" fill="#2A2A4A"/>
  <text x="60" y="167" fill="#FF4B8A" font-family="monospace" font-size="8">Microsoft - Cloud Engineer</text>
  <circle cx="320" cy="165" r="3" fill="#FF9500"/>
  <text x="330" y="167" fill="#999" font-family="monospace" font-size="6">Interview</text>
  
  <!-- Job Item 3 -->
  <rect x="55" y="180" width="290" height="20" rx="2" fill="#2A2A4A"/>
  <text x="60" y="192" fill="#FF4B8A" font-family="monospace" font-size="8">Amazon - ML Engineer</text>
  <circle cx="320" cy="190" r="3" fill="#DC382D"/>
  <text x="330" y="192" fill="#999" font-family="monospace" font-size="6">Rejected</text>
  
  <!-- Add Button -->
  <rect x="50" y="215" width="60" height="20" rx="4" fill="#FF4B8A" opacity="0.2"/>
  <text x="80" y="227" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">+ Add Job</text>
  
  <!-- Title -->
  <text x="200" y="250" fill="#FF4B8A" font-family="sans-serif" font-size="16" font-weight="bold" text-anchor="middle">Job Application Tracker</text>
  <text x="200" y="270" fill="#999" font-family="sans-serif" font-size="12" text-anchor="middle">Next.js • Supabase • Notion-style UI</text>
</svg>
