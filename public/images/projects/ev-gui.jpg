<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0B0B0D"/>
  <rect x="20" y="20" width="360" height="260" rx="12" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  
  <!-- EV Dashboard Display -->
  <rect x="40" y="40" width="320" height="180" rx="8" fill="#0F0F0F"/>
  
  <!-- Main Display Area -->
  <rect x="50" y="50" width="300" height="160" rx="6" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  
  <!-- Speedometer -->
  <circle cx="120" cy="100" r="40" fill="none" stroke="#FF4B8A" stroke-width="3"/>
  <circle cx="120" cy="100" r="35" fill="none" stroke="#333" stroke-width="2"/>
  <line x1="120" y1="100" x2="120" y2="70" stroke="#FF4B8A" stroke-width="2"/>
  <text x="120" y="140" fill="#FF4B8A" font-family="monospace" font-size="12" text-anchor="middle">65 km/h</text>
  
  <!-- Battery Level -->
  <rect x="200" y="60" width="120" height="30" rx="4" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  <rect x="205" y="65" width="80" height="20" fill="#00D4AA" opacity="0.8"/>
  <text x="260" y="80" fill="#FF4B8A" font-family="monospace" font-size="10" text-anchor="middle">78%</text>
  
  <!-- Temperature Gauge -->
  <rect x="200" y="100" width="120" height="30" rx="4" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  <rect x="205" y="105" width="60" height="20" fill="#FF9500" opacity="0.8"/>
  <text x="260" y="120" fill="#FF4B8A" font-family="monospace" font-size="10" text-anchor="middle">42°C</text>
  
  <!-- Status Indicators -->
  <circle cx="60" cy="60" r="8" fill="#00D4AA"/>
  <text x="60" y="80" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">READY</text>
  
  <circle cx="60" cy="100" r="8" fill="#FF4B8A"/>
  <text x="60" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">MOTOR</text>
  
  <circle cx="60" cy="140" r="8" fill="#FF9500"/>
  <text x="60" y="160" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">FAULT</text>
  
  <!-- Control Buttons -->
  <rect x="50" y="180" width="60" height="20" rx="4" fill="#FF4B8A" opacity="0.2"/>
  <text x="80" y="192" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">E-STOP</text>
  
  <rect x="120" y="180" width="60" height="20" rx="4" fill="#333"/>
  <text x="150" y="192" fill="#999" font-family="monospace" font-size="8" text-anchor="middle">RESET</text>
  
  <rect x="190" y="180" width="60" height="20" rx="4" fill="#333"/>
  <text x="220" y="192" fill="#999" font-family="monospace" font-size="8" text-anchor="middle">CALIB</text>
  
  <!-- Title -->
  <text x="200" y="250" fill="#FF4B8A" font-family="sans-serif" font-size="16" font-weight="bold" text-anchor="middle">EV Control GUI</text>
  <text x="200" y="270" fill="#999" font-family="sans-serif" font-size="12" text-anchor="middle">FreeRTOS • TM4C1294 • Touchscreen</text>
</svg>
