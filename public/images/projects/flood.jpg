<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0B0B0D"/>
  <rect x="20" y="20" width="360" height="260" rx="12" fill="#1A1A1A" stroke="#333" stroke-width="1"/>
  
  <!-- AI Processing Pipeline -->
  <rect x="40" y="40" width="320" height="180" rx="8" fill="#0F0F0F"/>
  
  <!-- Input Image -->
  <rect x="50" y="50" width="80" height="60" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <rect x="55" y="55" width="70" height="50" fill="#2A2A4A"/>
  <circle cx="70" cy="70" r="8" fill="#4A90E2" opacity="0.7"/>
  <circle cx="100" cy="85" r="6" fill="#4A90E2" opacity="0.5"/>
  <text x="90" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">CCTV Input</text>
  
  <!-- Arrow -->
  <path d="M140 80 L160 80" stroke="#FF4B8A" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- DeepLabv3+ Processing -->
  <rect x="170" y="50" width="80" height="60" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <rect x="175" y="55" width="70" height="50" fill="#2A2A4A"/>
  <rect x="180" y="60" width="60" height="40" fill="#FF4B8A" opacity="0.3"/>
  <text x="210" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">DeepLabv3+</text>
  
  <!-- Arrow -->
  <path d="M260 80 L280 80" stroke="#FF4B8A" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- SRGAN Enhancement -->
  <rect x="290" y="50" width="80" height="60" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <rect x="295" y="55" width="70" height="50" fill="#2A2A4A"/>
  <rect x="300" y="60" width="60" height="40" fill="#00D4AA" opacity="0.3"/>
  <text x="330" y="120" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">SRGAN</text>
  
  <!-- GIS Map Output -->
  <rect x="50" y="140" width="320" height="60" rx="4" fill="#1A1A2E" stroke="#255, 75, 138" stroke-width="1"/>
  <rect x="60" y="150" width="300" height="40" fill="#2A2A4A"/>
  <rect x="70" y="160" width="80" height="20" fill="#FF4B8A" opacity="0.4"/>
  <rect x="160" y="160" width="60" height="20" fill="#FF4B8A" opacity="0.6"/>
  <rect x="230" y="160" width="100" height="20" fill="#FF4B8A" opacity="0.3"/>
  <text x="200" y="220" fill="#FF4B8A" font-family="monospace" font-size="8" text-anchor="middle">GIS Flood Map</text>
  
  <!-- Title -->
  <text x="200" y="250" fill="#FF4B8A" font-family="sans-serif" font-size="16" font-weight="bold" text-anchor="middle">AI Flood Detection</text>
  <text x="200" y="270" fill="#999" font-family="sans-serif" font-size="12" text-anchor="middle">PyTorch • DeepLabv3+ • PostGIS</text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#FF4B8A"/>
    </marker>
  </defs>
</svg>
